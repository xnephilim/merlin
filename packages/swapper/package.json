{
  "name": "@xblackfury/swapper",
  "version": "18.0.0",
  "description": "An interface and implementations to trade coins via various protocols",
  "repository": "https://github.com/xnephilim/lib",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "dev": "tsc --watch",
    "clean": "rm -rf dist && rm -rf tsconfig.build.tsbuildinfo",
    "compile": "tsc -p tsconfig.build.json",
    "prepare": "yarn build",
    "test": "jest test",
    "type-check": "tsc --project ./tsconfig.build.json --noEmit",
    "swapcli": "yarn build && node ./dist/swappercli.js"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "axios-cache-adapter": "^2.7.3",
    "bignumber.js": "^9.1.1",
    "lodash": "^4.17.21",
    "retry-axios": "^2.6.0",
    "web3": "1.7.4"
  },
  "peerDependencies": {
    "@shapeshiftoss/hdwallet-core": "^1.46.0",
    "@shapeshiftoss/types": "^8.1.0",
    "@xblackfury/asset-service": "^8.0.1",
    "@xblackfury/caip": "^8.0.0",
    "@xblackfury/chain-adapters": "^11.0.0",
    "@xblackfury/errors": "^1.1.2"
  },
  "devDependencies": {
    "@shapeshiftoss/hdwallet-core": "^1.46.0",
    "@shapeshiftoss/types": "^9.0.0",
    "@types/readline-sync": "^1.4.4",
    "@xblackfury/asset-service": "^9.0.0",
    "@xblackfury/caip": "^9.0.0",
    "@xblackfury/chain-adapters": "^12.0.0",
    "@xblackfury/errors": "^2.0.0",
    "@xblackfury/hightable-client": "^11.0.0",
    "readline-sync": "^1.4.10",
    "web3-utils": "1.7.4"
  }
}
